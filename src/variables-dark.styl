dark = #484b4c
dark-blue = #e0f0fa
dark-border = 1px solid #2d2f31
dark-border-focus = 1px solid #2e301
dark-border-glow = 0 0 0 2px #6086a7
dark-button-divider = 1px solid #2d2f31
dark-down = #404141
dark-down-shadow = inset 0 1px 0px rgba(0,0,0,.18)
dark-gradient-start = #595b5b
dark-gradient-end = #595b5b
dark-gradient-highlight = inset 0 1px 0 rgba(255,255,255,.15)
dark-hover-gradient-start = #646666
dark-hover-gradient-end = #646666
dark-hud-shadow = 0 3px 9px rgba(0,0,0,.3)
dark-link-color = #5dc1ff
dark-link-glow = 0 0 0 2px #6086a7
dark-text = #c5c7c7
dark-text-shadow = 0 -1px 0 rgba(0,0,0,.69)
dark-track-background = #414545
dark-track-border = 1px solid #2d2f31

dark-quiet-border = 1px solid rgba(0,0,0,0)
dark-quiet-border-focus = 1px solid rgba(9,64,253,.84)
dark-quiet-border-glow = 0 0 4px 0 rgba(0,136,255,.69)
dark-quiet-gradient-start = rgba(0,0,0,0)
dark-quiet-gradient-end = rgba(0,0,0,0)
dark-quiet-gradient-highlight = inset 0 1px 0 rgba(0,0,0,0)
dark-quiet-hover-gradient-start = #646666
dark-quiet-hover-gradient-end = #646666
dark-quiet-down = #2b2d2d
dark-quiet-down-shadow = inset 0 1px 0px rgba(0,0,0,.06)
dark-quiet-text = #c5c7c7
dark-quiet-text-shadow = 0 -1px 0 rgba(0,0,0,.69)

var-background-color = #595b5b
var-background-color--down = #404141
var-border = 1px solid rgba(0, 0, 0, 0.36)
var-box-shadow = inset 0 1px rgba(255, 255, 255, 0.12)
var-box-shadow--down = inset 0 1px rgba(0, 0, 0, 0.18)
var-color = #c6c8c8
var-text-shadow =  0 -1px rgba(0, 0, 0, 0.69)
/* Call To Action */
var-background-color--cta--down = #1976c3
var-box-shadow--cta--down = inset 0 1px rgba(0, 0, 0, 0.12)

